{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "AirBnBADF"
		},
		"AirBnB_Synapse_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AirBnB_Synapse'"
		},
		"AirBnb_cosmosDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AirBnb_cosmosDB'"
		},
		"AirBnBAdls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://airbnbstracc.dfs.core.windows.net/"
		},
		"AirBnB_Synapse_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "airbnb-synapse-worspace.sql.azuresynapse.net"
		},
		"AirBnB_Synapse_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AirBnB_Sql_pool"
		},
		"AirBnB_Synapse_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "AirBnBADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AirBnB_Synapse_password": {
						"value": "[parameters('AirBnB_Synapse_password')]"
					},
					"AirBnb_cosmosDB_connectionString": {
						"value": "[parameters('AirBnb_cosmosDB_connectionString')]"
					},
					"AirBnBAdls_properties_typeProperties_url": {
						"value": "[parameters('AirBnBAdls_properties_typeProperties_url')]"
					},
					"AirBnB_Synapse_properties_typeProperties_server": {
						"value": "[parameters('AirBnB_Synapse_properties_typeProperties_server')]"
					},
					"AirBnB_Synapse_properties_typeProperties_database": {
						"value": "[parameters('AirBnB_Synapse_properties_typeProperties_database')]"
					},
					"AirBnB_Synapse_properties_typeProperties_userName": {
						"value": "[parameters('AirBnB_Synapse_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}